CREATE TABLE `realestate`.`housing` ( `id_housing` INT(5) NOT NULL AUTO_INCREMENT , `title` VARCHAR(30) NOT NULL , `address` VARCHAR(50) NOT NULL , `city` VARCHAR(30) NOT NULL , `pc` INT(8) NOT NULL , `area` INT(8) NOT NULL , `price` INT(12) NOT NULL , `photo` VARCHAR(50) NOT NULL , `type` BOOLEAN NOT NULL , `description` VARCHAR NOT NULL , PRIMARY KEY (`id_housing`)) ENGINE = MyISAM; 